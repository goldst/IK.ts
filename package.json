{
    "name": "ikts",
    "version": "1.3.7",
    "description": "Inverse Kinematics Library based on the FABRIC algorithm",
    "main": "build/IK.js",
    "repository": "goldst/IK.ts",
    "jsnext:main": "build/ik.js",
    "module": "build/IK.js",
    "files": [
        "package.json",
        "LICENSE",
        "README.md",
        "build/*"
    ],
    "directories": {
        "example": "examples"
    },
    "eslintConfig": {
        "extends": "mdcs"
    },
