    "bugs": {
        "url": "https://github.com/goldst/IK.ts/issues"
    },
    "homepage": "https://goldst.dev/IK.ts",
    "devDependencies": {
        "@types/three": "^0.138.0",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "eslint": "^8.10.0",
        "eslint-config-mdcs": "latest",
        "ts-loader": "^9.2.7",
        "typescript": "^4.6.2",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4"
    },
    "types": "build/IK.d.ts"
